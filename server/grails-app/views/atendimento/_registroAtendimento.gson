import br.com.hospitaldocoracaoal.integracao.Atendimento

model {
    Atendimento atendimento
}

json g.render(atendimento, [expand: ['motivoAlta', 'ecg', 'balao'],excludes: ['paciente', 'setor'], resolveTemplate: false]) {
    paciente g.render(atendimento.paciente, [excludes: 'atendimentos', resolveTemplate: false])
    setor g.render(atendimento.setor, [expand:'setor', excludes: 'atendimentos', resolveTemplate: false])
}
