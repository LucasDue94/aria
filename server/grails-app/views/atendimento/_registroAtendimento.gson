import br.com.hospitaldocoracaoal.integracao.Atendimento

model {
    Atendimento registroAtendimento
}

json g.render(registroAtendimento, [expand: ['motivoAlta', 'ecg', 'balao'],excludes: ['paciente', 'setor'], resolveTemplate: false]) {
    paciente g.render(registroAtendimento.paciente, [excludes: 'registrosAtendimento', resolveTemplate: false])
    setor g.render(registroAtendimento.setor, [expand:'setor', excludes: 'registrosAtendimento', resolveTemplate: false])
}
